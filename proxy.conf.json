{
    "/api/lostobject": {
      "target": "http://localhost:5001",
      "secure": false,
      "logLevel": "debug",
      "changeOrigin": true,
      "pathRewrite": {
        "^/api": ""
      }
    },
    "/api/auth/*": {
        "target": "http://localhost:5002",
        "secure": true,
        "logLevel": "debug",
        "changeOrigin": true,
        "pathRewrite": {
          "^/api/auth/": "/api/auth/login"
        }
      },
    "/api/zone/*": {
        "target": "http://localhost:5001",
        "secure": false,
        "logLevel": "debug",
        "changeOrigin": true,
        "pathRewrite": {
          "^/api": ""
        }
      }
}